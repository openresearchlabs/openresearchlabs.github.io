@article{Slonim05,
  author =	 {Slonim, Noam and Atwal, Gurinder Singh and Tkacik,
                  Gasper and Bialek, William},
  title =	 {{Information-based clustering}},
  journal =	 {Proceedings of the National Academy of Sciences,
                  USA},
  volume =	 102,
  number =	 51,
  pages =	 {18297-18302},
  doi =		 {10.1073/pnas.0507432102},
  year =	 2005,
  abstract =	 {Existing clustering methods typically depend on
                  nontrivial assumptions about the structure of
                  data. Here, we reformulate the clustering problem
                  from an information theoretic perspective that
                  avoids many of these assumptions like the need for
                  defining a cluster "prototype," and a priori
                  similarity metric, is invariant to changes in the
                  representation of the data, and naturally captures
                  nonlinear relations. Finally, our approach provides
                  a way of clustering based on collective notions of
                  similarity rather than the traditional pairwise
                  measures},
  URL =
                  {http://www.pnas.org/cgi/content/abstract/102/51/18297},
  eprint =	 {http://www.pnas.org/cgi/reprint/102/51/18297.pdf}
}

