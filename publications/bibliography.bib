journal = {PLoS computational biology},
keywords = {Computational Biology,Computational Biology/Genomics,Computational Biology/Systems Biology,Computational Biology/Transcriptional Regulation,Genetics and Genomics,Genetics and Genomics/Gene Expression,Mathematics/Statistics,Research Article},
