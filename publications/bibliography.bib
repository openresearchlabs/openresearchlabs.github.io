issn = {1932-6203},
journal = {PLoS ONE},
keywords = {Computer Science/General and Management Topics,Computer Science/Information Technology,Research Article,Science Policy,Science Policy/Education,open access publishing statistics publication},
mendeley-tags = {open access publishing statistics publication},
month = oct,
number = {10},
pages = {e13636},
