journal = {PLoS Computational Biology},
keywords = {Computational Biology/Genomics,Computational Biology/Molecular Genetics,Computational Biology/Population Genetics,Genetics and Genomics/Bioinformatics,Genetics and Genomics/Epigenetics,Genetics and Genomics/Gene Expression,Genetics and Genomics/Genomics,Research Article},
month = jul,
number = {7},
pages = {12},
publisher = {Public Library of Science},
title = {{Computational Analysis of Whole-Genome Differential Allelic Expression Data in Human}},
