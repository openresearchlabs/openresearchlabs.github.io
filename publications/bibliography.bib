keywords = {human mouse gene expression comparative functional},
mendeley-tags = {human mouse gene expression comparative functional},
month = dec,
number = {12},
pages = {R124},
pmid = {21182765},
title = {{Large scale comparison of global gene expression patterns in human and mouse.}},
url = {http://genomebiology.com/2010/11/12/R124},
volume = {11},
year = {2010}
}
