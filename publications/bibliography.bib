issn = {1932-6203},
journal = {PLoS ONE},
keywords = {Computational Biology/Evolutionary Modeling,Mathematics/Game Theory,Physics/Interdisciplinary Physics,Research Article,cooperation,dynamic,evolution,network,stochastic},
mendeley-tags = {cooperation,dynamic,evolution,network,stochastic},
