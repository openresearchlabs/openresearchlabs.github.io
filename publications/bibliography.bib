issn = {1462-2920},
journal = {Environmental microbiology},
keywords = {Adult,Aged,Biodiversity,DNA,Feces,Feces: microbiology,Gastrointestinal Tract,Gastrointestinal Tract: microbiology,Genetic Variation,HITChip,Humans,Metagenome,Microarray Analysis,Microarray Analysis: methods,Microbiological Techniques,Microbiological Techniques: methods,Oligonucleotide Probes,Oligonucleotide Probes: genetics,Reproducibility of Results,Ribosomal,Ribosomal: genetics},
