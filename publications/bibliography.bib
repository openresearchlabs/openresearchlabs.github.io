doi = {10.1111/j.1462-2920.2009.01982.x},
issn = {1462-2920},
journal = {Environmental microbiology},
keywords = {Bacteroides,Bacteroides: genetics,Bacteroides: isolation \& purification,Bifidobacterium,Bifidobacterium: genetics,Bifidobacterium: isolation \& purification,Biodiversity,DNA, Bacterial,DNA, Bacterial: genetics,DNA, Bacterial: isolation \& purification,Eubacterium,Eubacterium: genetics,Eubacterium: isolation \& purification,Feces,Feces: microbiology,Genome, Bacterial,Gram-Positive Bacteria,Gram-Positive Bacteria: genetics,Gram-Positive Bacteria: isolation \& purification,Humans,Intestines,Intestines: microbiology,Phylogeny,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: analysis,RNA, Ribosomal, 16S: genetics,Ruminococcus,Ruminococcus: genetics,Ruminococcus: isolation \& purification,Sequence Analysis, DNA,Species Specificity},
