file = {:home/antagomir/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kalaitzis, Lawrence - 2011 - A Simple Approach to Ranking Differentially Expressed Gene Expression Time Courses through Gaussian Process Regression.pdf:pdf},
issn = {1471-2105},
journal = {BMC bioinformatics},
keywords = {gene expression time series regression Gaussian pr},
mendeley-tags = {gene expression time series regression Gaussian pr},
month = may,
number = {1},
pages = {180},
pmid = {21599902},
title = {{A Simple Approach to Ranking Differentially Expressed Gene Expression Time Courses through Gaussian Process Regression.}},
url = {http://www.biomedcentral.com/1471-2105/12/180},
volume = {12},
