journal = {PLoS ONE},
keywords = {Algorithms,Biochemistry,Biology,Biomarkers,Breast cancer,Breast tumors,Cancers and neoplasms,Computational Biology,Computer Science,Computer inferencing,Computing methods,Diagnostic medicine,Ductal carcinoma in situ,Gene expression,General pathology,Genetics,Genetics and Genomics,Medicine,Microarrays,Molecular Biology,Molecular cell biology,Molecular genetics,Obstetrics and gynecology,Oncology,Pathology,Protein interactions,Proteins,Proteomics,Research Article,biomarker,network,signature},
