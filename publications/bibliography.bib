journal = {PLoS One},
keywords = {Biochemistry/Bioinformatics,Genetics and Genomics/Cancer Genetics,Genetics and Genomics/Epigenetics,Oncology/Breast Cancer,Research Article,cgh,comparison,copy number,gene expression,methylation},
mendeley-tags = {cgh,comparison,copy number,gene expression,methylation},
month = jan,
number = {1},
pages = {e8665},
pmid = {20084286},
publisher = {Public Library of Science},
title = {{Multi-platform whole-genome microarray analyses refine the epigenetic signature of breast cancer metastasis with gene expression and copy number.}},
