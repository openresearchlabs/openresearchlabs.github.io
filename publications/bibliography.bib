journal = {The ISME journal},
keywords = {16S,Algorithms,Animals,Archaea,Archaea: classification,Archaea: genetics,Archaea: isolation \& purification,Bacteria,Bacteria: classification,Bacteria: genetics,Bacteria: isolation \& purification,Bayes Theorem,DNA, Archaeal,DNA, Archaeal: genetics,DNA, Bacterial,DNA, Bacterial: genetics,Gastrointestinal Tract,Gastrointestinal Tract: microbiology,High-Throughput Nucleotide Sequencing,Humans,Metagenome,Mice,Phylogeny,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Ribotyping,Ribotyping: methods,database,greengenes},
