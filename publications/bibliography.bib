keywords = {Breast Neoplasms,Breast Neoplasms: genetics,Chromosome Aberrations,Disease Progression,Gene Dosage,Genome,Humans,Oligonucleotide Array Sequence Analysis,RNA, Messenger,RNA, Messenger: metabolism,Transcription, Genetic,Tumor Cells, Cultured,cgh,data,expression,integration,pint,simcca},
