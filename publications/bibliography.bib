mendeley-tags = {cgh,expression,integration,pathway,pint,pro,simcca},
month = jun,
number = {3},
pages = {575--89},
pmid = {19688261},
title = {{An integrative genomic and transcriptomic analysis reveals molecular pathways and networks regulated by copy number aberrations in basal-like, HER2 and luminal cancers.}},
url = {http://www.springerlink.com/content/m476160l161881h7},
volume = {121},
year = {2010}
