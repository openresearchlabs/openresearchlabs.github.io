issn = {1932-6203},
journal = {PLoS ONE},
keywords = {Biology,Biostatistics,Computational Biology,Genetics and Genomics,Genome expression analysis,Genomics,Mathematics,Microarrays,Research Article,Statistics,pint-jatko retrieval10 factor analysis expression},
mendeley-tags = {pint-jatko retrieval10 factor analysis expression},
month = mar,
number = {3},
pages = {e17691},
publisher = {Public Library of Science},
title = {{Unifying Gene Expression Measures from Multiple Platforms Using Factor Analysis}},
