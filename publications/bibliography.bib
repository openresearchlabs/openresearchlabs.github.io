file = {:home/antagomir/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Tsafrir et al. - 2006 - Relationship of gene expression and chromosomal abnormalities in colorectal cancer.pdf:pdf},
issn = {0008-5472},
