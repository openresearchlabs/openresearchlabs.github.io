keywords = {16s rrna, 16s ribosomal ribonucleic acid,bv, bacterial vaginosis,bvab, bacterial vaginosis-associated bacteria,dna, deoxyribonucleic acid,ecology,h2o2, hydrogen peroxide,hiv, human immunodeficiency virus,koh, potassium hydroxide,microbiome,vagina},
mendeley-tags = {ecology,microbiome,vagina},
