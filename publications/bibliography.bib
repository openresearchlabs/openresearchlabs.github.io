issn = {1362-4962},
journal = {Nucleic acids research},
keywords = {bacterial,database,genome,microbiome},
mendeley-tags = {bacterial,database,genome,microbiome},
month = dec,
number = {D1},
pages = {D599--604},
pmid = {22135301},
title = {{BacMap: an up-to-date electronic atlas of annotated bacterial genomes.}},
url = {http://nar.oxfordjournals.org/cgi/content/abstract/40/D1/D599},
volume = {40},
year = {2011}
}
