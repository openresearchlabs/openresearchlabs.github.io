keywords = {alignment,interpretation,ngs,taxonomy},
mendeley-tags = {alignment,interpretation,ngs,taxonomy},
month = aug,
number = {35},
pages = {14637--14642},
title = {{Bacterial community comparisons by taxonomy-supervised analysis independent of sequence alignment and clustering}},
url = {http://www.pnas.org/cgi/content/abstract/108/35/14637},
volume = {108},
year = {2011}
}
