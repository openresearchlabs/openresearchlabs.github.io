journal = {PloS one},
keywords = {Algorithms,Biology,Biology: methods,Computer Simulation,Diffusion,Ecology,Ecology: methods,Environment,Lakes,Models, Biological,Models, Statistical,Models, Theoretical,Oceans and Seas,Reproducibility of Results,Systems Theory,Time Factors,bifurcation,catastrophe,chaos,dynamical systems,early warning signals,ecosystem,resilience},
mendeley-tags = {bifurcation,catastrophe,chaos,dynamical systems,early warning signals,ecosystem,resilience},
