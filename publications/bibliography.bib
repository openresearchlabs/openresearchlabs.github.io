number = {1},
pages = {84},
title = {{Protein interaction network topology uncovers melanogenesis regulatory network components within functional genomics datasets}},
url = {http://www.biomedcentral.com/1752-0509/4/84},
volume = {4},
year = {2010}
}
@article{Hesse2010,
abstract = {ABSTRACT: Substances, such as alcohol, opiates and cannabis, have been used by humans for millenniums. Today, a much wider range of substances are used for a range of purposes, including the enhancement of performance during university studies, sexual experiences, sports and exercise, and celebration, socializing and the experience of art and music. Substance use is also associated with a range of harms to the individual and society. Prohibitions, regulation, prevention and treatment are all used to prevent this harm. In this commentary, it is argued that the task of public health interventions should be to target relevant harms, but not to evaluate which aspects of human endeavors and experiences that should be enhanced, and which should not. It is argued that interventions should directly target the harms, using the best available evidence concerning mechanisms underlying harms. Two examples are given of substances that may be altered to prevent serious harm, one for alcohol and one for cannabis. In the case of alcohol, the addition of dissolved oxygen could reduce both the risk of accidents and the risk of liver damage associated with alcohol consumption. In the case of cannabis, there is strong indication that the reduction of content Delta-tetrahydrocannabinol and increase of cannabidiol could reduce the risk of psychoses and possibly addiction associated with cannabis use. The aim is to show that responsible regulation should not necessarily be restricted to preventing use and (in the case of alcohol) reduction of amounts and frequency, but can potentially include a range of other strategies that can reduce the burden of illness associated with substance use.},
author = {Hesse, Morten},
doi = {10.1186/1741-7015-8-50},
file = {:home/antagomir/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Hesse - 2010 - Enhancement drugs are there limits to what we should enhance and why.pdf:pdf},
