title = {{The function of our microbiota: who is out there and what do they do?}},
url = {http://www.frontiersin.org/Journal/FullText.aspx?s=149\&name=Cellular\_and\_Infection\_Microbiology\&ART\_DOI=10.3389/fcimb.2012.00104},
